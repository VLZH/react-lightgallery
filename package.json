{
    "name": "react-lightgallery",
    "version": "0.2.2",
    "description": "React wrapper for https://github.com/sachinchoolur/lightgallery.js",
    "keywords": [
        "react",
        "gallery",
        "image-gallery",
        "lightbox",
        "lightgallery.js",
        "lg-zoom.js",
        "lg-thumbnail.js",
        "lg-video.js",
        "lightgallery"
    ],
    "main": "dist/index.js",
    "scripts": {
        "build": "NODE_ENV=production yarn rollup -c ./rollup.config.js",
        "start": "yarn rollup --watch -c ./rollup.config.js",
        "lint": "yarn eslint ./"
    },
    "author": "VLZH",
    "repository": "https://github.com/VLZH/react-lightgallery.git",
    "license": "GPLv3",
    "dependencies": {
        "browser-or-node": "^1.1.0",
        "lg-fullscreen.js": "^1.1.0",
        "lg-zoom.js": "^1.0.1",
        "lg-thumbnail.js": "^1.1.0",
        "lg-video.js": "^1.0.0",
        "lightgallery.js": "^1.1.3",
        "lodash": "^4.17.11",
        "prop-types": "^15.7.2",
        "react": "^16.8.3",
        "react-dom": "^16.8.3",
        "uniqid": "^5.0.3"
    },
    "devDependencies": {
        "@babel/core": "^7.3.4",
        "@babel/plugin-proposal-class-properties": "^7.3.4",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/preset-env": "^7.3.4",
        "@babel/preset-react": "^7.0.0",
        "babel-eslint": "9.x.x",
        "eslint": "^5.15.1",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.12.4",
        "husky": "^1.3.1",
        "prettier": "^1.16.4",
        "rollup": "^1.4.1",
        "rollup-plugin-babel": "^4.3.2",
        "rollup-plugin-commonjs": "^9.2.1",
        "rollup-plugin-node-resolve": "^4.0.1",
        "rollup-plugin-terser": "^4.0.4",
        "rollup-plugin-uglify": "^6.0.2"
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ],
        "plugins": [
            "@babel/plugin-proposal-class-properties",
            "@babel/plugin-syntax-dynamic-import"
        ]
    },
    "prettier": {
        "tabWidth": 4
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn run eslint"
        }
    }
}
